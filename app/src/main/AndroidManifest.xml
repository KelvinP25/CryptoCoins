<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.cryptocoins">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:usesCleartextTraffic="true"
        android:name=".CryptoCoinApp"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/btc_crypto_cryptocurrency_cryptocurrencies_cash_money_bank_payment_95084"
        android:label="@string/app_name"
        android:roundIcon="@drawable/btc_crypto_cryptocurrency_cryptocurrencies_cash_money_bank_payment_95084"
        android:supportsRtl="true"
        android:theme="@style/Theme.CryptoCoins"
        tools:targetApi="31">
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.CryptoCoins">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>